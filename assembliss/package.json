{
  "name": "assembliss",
  "displayName": "assembliss",
  "description": "Emulate and debug ARMv8 assembly in VS Code",
  "version": "0.0.1",
  "publisher": "NCSU",
  "repository": "https://github.ncsu.edu/engr-csc-sdc/2024SpringTeam37-Batista.git",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onDebugResolve:qdb",
    "onDebug",
    "onDebugResolve:arm"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-assembliss"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/path-browserify": "^1.0.2",
    "@types/vscode": "^1.87.0",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vscode/debugadapter": "^1.65.0",
    "@vscode/debugprotocol": "^1.65.0",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "await-notify": "^1.0.1",
    "eslint": "^8.56.0",
    "tslint": "^5.16.0",
    "typescript": "^5.3.3"
  },
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Assembliss",
      "properties": {
        "assembliss.assembler": {
          "title": "Assembler",
          "type": "string",
          "default": "aarch64-linux-gnu-as",
          "description": "Assembler used by assembliss commands. For example, 'aarch64-linux-gnu-as' or full path if not on system path."
        },
        "assembliss.linker": {
          "title": "Linker",
          "type": "string",
          "default": "aarch64-linux-gnu-ld",
          "description": "Linker used by assembliss commands. For example, 'aarch64-linux-gnu-ld' or full path if not on system path."
        }
      }
    },
    "commands": [
      {
        "command": "assembliss.getProgramName",
        "title": "Assembliss: Get Program Name"
      },
      {
        "command": "assembliss.runEditorContents",
        "title": "Assembliss: Run Editor Contents"
      },
      {
        "command": "assembliss.debugEditorContents",
        "title": "Assembliss: Debug Editor Contents"
      },
      {
        "command": "assembliss.toggleFormatting",
        "title": "Assembliss: Toggle Formatting"
      },
      {
        "command": "assembliss.AS",
        "title": "Assembliss: AS"
      },
      {
        "command": "assembliss.LD",
        "title": "Assembliss: LD"
      }
    ],
    "breakpoints": [
      {
        "language": "arm64"
      }
    ],
    "debuggers": [
      {
        "type": "qdb",
        "program": "out/Debugger/Qdb.js",
        "runtime": "node",
        "label": "Assembliss",
        "languages": [
          "arm64",
          "arm",
          "aarch64",
          "armv8",
          "armv8-a",
          "arm64-a",
          "asm",
          "assembly",
          "arm assembly",
          "armv8 assembly",
          "arm64 assembly",
          "aarch64 assembly",
          "ARMv8 Assembly",
          "qdb"
        ],
        "configurationAttributes": {
          "launch": {
            "required": [
              "target"
            ],
            "properties": {
              "target": {
                "type": "string",
                "description": "Path of executable",
                "default": "${workspaceFolder}/${command:AskForProgramName}"
              },
              "stopOnEntry": {
                "type": "boolean",
                "description": "Automatically stop after launch.",
                "default": true
              },
              "trace": {
                "type": "boolean",
                "description": "Enable logging of the Debug Adapter Protocol.",
                "default": true
              }
            }
          }
        },
        "initialConfigurations": [
          {
            "type": "qdb",
            "request": "launch",
            "name": "Assembliss: Ask for file name (Initial Config)",
            "target": "${workspaceFolder}/${command:AskForProgramName}",
            "stopOnEntry": true
          }
        ],
        "configurationSnippets": [
          {
            "label": "Assembliss: Launch",
            "description": "A new configuration for debugging a user selected assembly file.",
            "body": {
              "type": "qdb",
              "request": "launch",
              "name": "Assembliss: Ask for program name",
              "target": "^\"\\${workspaceFolder}/\\${command:AskForProgramName}\"",
              "stopOnEntry": true
            }
          }
        ],
        "variables": {
          "AskForProgramName": "assembliss.getProgramName"
        }
      }
    ],
    "languages": [
      {
        "id": "arm64",
        "aliases": [
          "ARMv8 Assembly",
          "ARM64 Assembly",
          "AArch64 Assembly",
          "arm64",
          "armv8",
          "aarch64",
          "ARMv8-A Assembly",
          "64-bit ARM Assembly",
          "ARM Assembly (ARMv8/ARM64)",
          "qdb"
        ],
        "extensions": [
          ".asm",
          ".s",
          ".arm"
        ],
        "configuration": "./language-configuration.json"
      }
    ]
  },
  "dependencies": {
    "@types/path-browserify": "^1.0.2"
  }
}
